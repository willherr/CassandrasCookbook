  {
    "type": "Dessert",
    "title": "Cinnamon Doughnut Holes",
    "introduction": "A Mary Kay recipe.",
    "imageUrl": "https://easyfood.ie/wp-content/uploads/2018/12/Cinnamon-doughnut-holes.jpg",
    "prepTime": 30,
    "totalTime": 60,
    "servings": 48,
    "ingredients": [
      {
        "title": "Flour",
        "amount": 1.75,
        "unitOfMeasure": "cups"
      },
      {
        "title": "Baking Powder",
        "amount": 1,
        "unitOfMeasure": "tsp"
      },
      {
        "title": "Cinnamon",
        "amount": 0.25,
        "unitOfMeasure": "tsp"
      },
      {
        "title": "Vegetable Oil"
      },
      {
        "title": "Sugar",
        "amount": 0.5,
        "unitOfMeasure": "cups"
      },
      {
        "title": "Melted Butter",
        "amount": 2,
        "unitOfMeasure": "tbsp"
      },
      {
        "title": "Egg",
        "amount": 1
      },
      {
        "title": "Buttermilk",
        "amount": 0.33,
        "unitOfMeasure": "cups"
      },
      {
        "title": "Salt",
        "amount": 1,
        "unitOfMeasure": "pinch"
      }
    ],
    "steps": [
      {
        "subtitle": "Prep",
        "instructions": "Mix dry ingredients (except sugar). Whisk sugar and butter. Now whisk in egg and buttermilk until smooth. Stir in dry ingredients with a spatula to form soft, pliable dough. Cover with plastic and refrigerate at least 30 minutes or overnight."
      },
      {
        "subtitle": "Fry",
        "instructions": "Heat 1 inch of oil in skillet to 325 degrees. Use spoon to form dough into balls. Drop dough balls into hot oil and fry until golden brown on both sides (about 1-2 minutes on each side). Drain doughnut holes on paper towel. Roll in cinnamon sugar or frosting while still warm."
      }
    ]
  },

  {
